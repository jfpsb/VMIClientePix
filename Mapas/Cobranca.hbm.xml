<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VMIClientePix"
                   namespace="VMIClientePix.Model">

	<class name="Cobranca" table="cobranca">
		<id name="Txid" />
		<many-to-one name="Calendario" class="VMIClientePix.Model.Calendario" cascade="save-update" />
		<many-to-one name="Valor" class="VMIClientePix.Model.Valor" cascade="save-update"/>
		<many-to-one name="Loc" class="VMIClientePix.Model.Loc" cascade="save-update"/>
		<many-to-one name="QrCode" class="VMIClientePix.Model.QRCode" cascade="save-update"/>
		<property name="Revisao" />
		<property name="Status" />
		<property name="Chave" />
		<property name="Location" />
		<property name="PagoEm" />
	</class>

</hibernate-mapping>
